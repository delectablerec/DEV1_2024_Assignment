@{
    ViewData["Title"] = "Home Page";
}

<main class="container my-4">
      <!-- Carousel Section -->
      <section class="mb-3">
        <div
          id="carouselExample"
          class="carousel slide"
          data-bs-ride="carousel"
        >
          <div class="carousel-inner">
            <!-- Example carousel items -->
            
            <div class="carousel-item active">
              <img
                src="data:image/png;base64,"
                class="d-block w-100"
                alt="Product 1"
              />
              <div class="carousel-caption d-none d-md-block">
                <h5>Product 1</h5>
                <p>Product description goes here.</p>
              </div>
            </div>
            <div class="carousel-item">
              <img
                src="data:image/png;base64,"
                class="d-block w-100"
                alt="Product 2"
              />
              <div class="carousel-caption d-none d-md-block">
                <h5>Product 2</h5>
                <p>Product description goes here.</p>
              </div>
              
            </div>
            <!-- Add more items as needed -->
            
          </div>
          <button
            class="carousel-control-prev"
            type="button"
            data-bs-target="#carouselExample"
            data-bs-slide="prev"
          >
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button
            class="carousel-control-next"
            type="button"
            data-bs-target="#carouselExample"
            data-bs-slide="next"
          >
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
        </div>
      </section>
      <div class="text-center p-5">
        <a class="btn btn-primary btn-lg" asp-area="" asp-controller="Products" asp-action="Index">
          <i class="bi bi-arrow-down-circle me-2" aria-hidden="true"></i>Scopri la collezione</a>
      </div>
      <!-- Cards Section -->
      <section id="cardshomepage" class="row text-center">
        <div class="col-lg-6">
          <div class="card " onclick="location.href='#'">
            <i class="bi bi-star-fill text-warning fs-1"></i>
            <h5 class="card-title mt-2">Most Sold Products</h5>
          </div>
        </div>
        <div class="col-lg-6">
          <div class="card " onclick="location.href='#'">
            <i class="bi bi-tag-fill text-success fs-1"></i>
            <h5 class="card-title mt-2">Cheapest Products</h5>
          </div>
        </div>
        <div class="w-100"></div>
        <div class="col-lg-6">
          <div class="card " onclick="location.href='#'">
            <i class="bi bi-graph-up-arrow text-info fs-1"></i>
            <h5 class="card-title mt-2">Total Products Sold: 1234</h5>
          </div>
        </div>
        <div class="col-lg-6">
          <div class="card " onclick="location.href='#'">
            <i class="bi bi-people-fill text-secondary fs-1"></i>
            <h5 class="card-title mt-2">Total Clients Number: 4536</h5>
          </div>
        </div>
      </section>
    </main>

